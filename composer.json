{
    "type": "library",
    "name": "kherge/collection-access",
    "description": "Manages access to collections from different types of sources.",
    "keywords": ["collection", "collections"],
    "license": [
        "MIT",
        "Apache-2.0"
    ],
    "support": {
        "issues": "https://github.com/kherge-php/collection-access/issues",
        "source": "https://github.com/kherge-php/collection-access"
    },

    "authors": [
        {
            "name": "Kevin Herrera",
            "email": "kevin@herrera.io",
            "homepage": "http://kevin.herrera.io/",
            "role": "Developer"
        }
    ],

    "autoload": {
        "psr-4": {
            "KHerGe\\CollectionAccess\\": "src/KHerGe/CollectionAccess"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Tests\\KHerGe\\CollectionAccess\\": "tests/KHerGe/CollectionAccess"
        }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },

    "require": {
        "php": ">=7.1",

        "doctrine/inflector": "^1.0",
        "psr/cache": "~1.0"
    }
}
